{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "control.html", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "manifest.json", "use": "@vercel/static" },
    { "src": "assets/**/*", "use": "@vercel/static" },
    { "src": "public/**/*", "use": "@vercel/static" },
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "save_manifest.php", "use": "@vercel/php" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/control", "dest": "/control.html" },
    { "src": "/control.html", "dest": "/control.html" },
    { "src": "/manifest.json", "dest": "/manifest.json" },
    { "src": "/save_manifest.php", "dest": "/save_manifest.php" },
    { "src": "/api/(.*)", "dest": "/api/$1.js" }
  ]
}
